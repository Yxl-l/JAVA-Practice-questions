<!--定义Mapper映射文件的约束和基本结构-->
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yxl.mapper.LoginMapper">

    <insert id="insert">
        insert into emp_login_log(username,password,login_time,is_success,jwt,cost_time) values (#{username},#{password},#{loginTime},#{isSuccess},#{jwt},#{costTime})
    </insert>
</mapper>